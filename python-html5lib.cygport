inherit python-wheel

NAME="python-html5lib"
VERSION=1.0.1
RELEASE=1
CATEGORY="Python"
SUMMARY="Python WHATWG HTML parser"
DESCRIPTION="html5lib is a pure-python library for parsing HTML. It is designed
to conform to the WHATWG HTML specification, as is implemented by all major web
browsers."

ARCH=noarch

python27_html5lib_REQUIRES="python27-chardet"
python36_html5lib_REQUIRES="python36-chardet"
python37_html5lib_REQUIRES="python37-chardet"
